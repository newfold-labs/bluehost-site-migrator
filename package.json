	"devDependencies": {
		"@reduxjs/toolkit": "^1.9.5",
		"@tailwindcss/forms": "^0.5.4",
		"@wordpress/dom-ready": "^3.40.0",
		"@wordpress/eslint-plugin": "^15.0.0",
		"@wordpress/prettier-config": "^2.23.0",
		"@wordpress/scripts": "^26.11.0",
		"autoprefixer": "^10.4.15",
